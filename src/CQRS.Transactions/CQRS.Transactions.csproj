<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Packable>true</Packable>
    <Authors>Bernhard Richter</Authors>
    <Description>Enables easy transaction management in CQRS style applications.</Description>
    <Copyright>Bernhard Richter</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/seesharper/CQRS.Transactions</PackageProjectUrl>
    <RepositoryUrl>https://github.com/seesharper/CQRS.Transactions</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>CQRS AspNetCore Dotnet Transactions</PackageTags>
    <DebugSymbols>true</DebugSymbols>
    <DebugType>portable</DebugType>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>
</Project>
